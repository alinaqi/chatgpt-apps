{
  "name": "chatgpt-apps",
  "version": "0.1.0",
  "description": "CLI tool to generate ChatGPT Apps from OpenAPI specs using agentic AI workflows",
  "main": "dist/index.js",
  "bin": {
    "chatgpt-apps": "./bin/chatgpt-apps.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "chatgpt",
    "openai",
    "mcp",
    "cli",
    "openapi",
    "ai",
    "code-generation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "yaml": "^2.4.0",
    "zod": "^3.23.0",
    "ejs": "^3.1.10",
    "express": "^4.19.0",
    "openapi-types": "^12.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/inquirer": "^9.0.7",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "typescript": "^5.4.0",
    "tsx": "^4.7.0",
    "vitest": "^1.4.0",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
